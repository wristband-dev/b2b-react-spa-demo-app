    "@mui/material": "^5.12.0",
    "@mui/x-date-pickers": "^6.1.0",
    "@tanstack/react-query": "^4.29.1",
    "@tanstack/react-query-devtools": "^4.29.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.5",
    "date-fns": "^2.29.3",
